page {
  height: 100vh;
  background: "#171717";
  font-size: 32px;
}

.wrapper {
  width: 100%;
  display: flex;
  flex-direction: column;
}

.search {
  display: flex;
  padding: 20rpx 30rpx;
  align-items: center;
  background-color: "white";
  border-width: 1rpx 0;
  border-color: #e9e9e9;
  border-style: solid;
  margin-bottom: 30rpx;
  font-size: 30rpx;
}
.search icon {
  margin-right: 30rpx;
}
.search-text{
  color: #e9e9e9;
}