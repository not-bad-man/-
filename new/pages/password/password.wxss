/* pages/password/password.wxss */
.wrapper{
  width: 100%;
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}
.head-sign,
.tail-sign{
  padding: 60rpx 0; 
  font-size: 32rpx;
  text-align: center;
  width: 100%;
}
.tail-sign view{
  margin-top: 16rpx;
}
.password{
  flex: 1;
  text-align: center;
  font-size: 240rpx;
  font-weight: bolder;
}
.sign{
  color: coral;
}
